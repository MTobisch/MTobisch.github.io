/* Stylesheet for mobile resolutions
----------------------------------------------------- */

/* Breakpoint 1600px 
-------------------------*/ 

@media (max-width: 1600px) { 
	
	#ls_config .ls_title {
		font-size: 1.0em;
		padding: 15px 0px;
	}
}

/* Breakpoint 1470px 
-------------------------*/ 

@media (max-width: 1470px) { 
	
	#ls_config .ls_title {
		display: none;
	}
}

/* Breakpoint 1200px 
-------------------------*/ 

@media (max-width: 1200px) { 
	
	#ls_config .ls_title {
		display: inline;
		font-size: 1.1em;
		margin: 0;
		padding: 15px 0px;
		text-align: center;
		color: white;
		position: absolute;
		right: 118px;
	}
	
	#ls_config #ls_menubutton-container {
		display: none;
	}
	#ls_safeModeDisplay {
		display: none;
	}
	#ls_config #ls_mobile_menu {
		display: inline;
	}
	
	#ls_config #ls_buildMenu {
		width: 200px;
	}
	
	#ls_config #ls_infowindow {
		width: 300px;
	}
	
	#ls_config #ls_infoWindow-title {
		width: 100%;
	}
	
	#ls_config #ls_infowindow_innercontainer_left {
		width: 100%;
	}
	
	#ls_config #ls_infowindow_innercontainer_right {
		display: none;
	}	
		#ls_config #ls_buildMenuDesktop_button_icon {
			margin: 0 2px 0 5px;
		}
		
		#ls_config #ls_buildMenuDesktop_button_title {
			padding: 6px 0px 0px 0px;
			font-size: 0.8em;
		}
	
	#ls_config #ls_select_container	 {
		width: 100%;
	}
	
		#ls_config #ls_select_menu_topcat_title {
			font-size: 1.2em;
			padding: 5px 10px 0 25px;
		}
		
		#ls_config .ls_select_menu_topcat_item {
			padding: 8px 10px 0 10px;
			font-size: 1.2em;
		}
		
		#ls_config #ls_select_menu_subcat {
			height: 45%;
		}
		
		#ls_config  .ls_select_menu_subcat_item {
			margin: 5px 0px 0px 5px;
		}
		
		#ls_config .ls_select_itemcontainer {
			width: 25%;
		}
	
	
		#ls_config #ls_select_inspect_modInnerContainer .ls_select_itemcontainer {
			width: 50%;
			height: 50%;
		}
			
		#ls_config #ls_select_inspect_ok_button {
			width: 391px;
		}
		
		#ls_config .ls_select_inspect_colorItem {
			width: 17%;
		}
	
	
	
	#ls_config #ls_config_container {
		width: 100%;
	}
	
	#ls_config_container_leftEdge {
		display: none;
	}
	
	#ls_config_container_rightEdge {
		display: none;
	}
	
	#ls_config #ls_config_innerContainer {
		float: none;
		margin: auto;
		width: 100%;
	}
	
	#ls_config .ls_config_panel_button {
		border-radius: 0px;
	}
	
	
}

/* Breakpoint 900px 
-------------------------*/ 
@media (max-width: 900px), (max-height: 500px) {
	
