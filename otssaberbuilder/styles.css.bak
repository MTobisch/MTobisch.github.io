@import 'mobile.css' (max-width: 1600px);

/* 1. General */
/* --------------------------------------------------------------------------------- */

body {
	height: 100%;
	margin: 0px;
	background-color: slategray;
	font-family: "Play";
}

#wrapper {
	min-width: 350px;
	margin: auto auto;
}


#ls_config {
	position: relative;
	width: 100%;
	height: 100%;
	background-size: 100%;
}

	#ls_config img {
		-webkit-user-select: none;
		-khtml-user-select: none;
		-moz-user-select: none;
		-o-user-select: none;
		user-select: none;
	}

#ls_menubar {
	position: relative;
	height: 50px;
	width: 100%;
	background-color: #404040;
	border-bottom: 2px solid black;
}

 .ls_title {
	font-size: 1.3em;
	margin: 0;
	padding: 10px 0px;
	text-align: center;
	color: white;
 }
 
 	#ls_select_container .ls_title {
 		font-size: 1.3em;
		padding: 2px 0px 0px 10px;
		text-align: left;
 	}
 	
/* 1.2 Loadingscreen */
/*-------------------*/	 

#ls_background_cover {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background-color: #404040;
	opacity: 1;
	transition: 1s ease all;
}

#ls_loadingscreen {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	z-index: 10000;
}

	#ls_loadingscreen_panel {
		position: absolute;
		top: 50%;
		left: 0;
		right: 0;
		width: 300px;
		height: 60px;
		margin: auto;
		transform: translateY(-50%);	
	}

	#ls_loadingscreen_icon {
		display: inline-block;
		vertical-align: top;
		width: 75px;
		height: 100%;
		background: url("assets/loadicon.gif");
		background-size: contain;
		background-repeat: no-repeat;
	}
	
	#ls_loadingscreen_text {
		display: inline-block;
		vertical-align: top;
		height: 100%;
		text-align: center;
		margin: 0px 0px 0px 20px;
		font-size: 1.5em;
		padding: 15px 0px 0px 0px;
		box-sizing: border-box;
		color: white;
	}

 	
/* 2. Overlay UI */
/* --------------------------------------------------------------------------------- */	
	
/* 2.1 Infowindow */
/*-----------------*/	

#ls_infowindow {
	position: absolute;
	top: 0px;
	width: 585px;
	height: 100px;
	overflow: hidden;
	z-index: 100;
}

	#ls_infowindow_innercontainer {
		float: left;
		height: 100%;
		width: calc(100% - 99px);
		background-color: orange;
		border-bottom: 2px solid black;
		box-sizing: border-box;
	}

	#ls_infowindow_edge {
		float: left;
		height: 100%;
		width: 99px;
		overflow: hidden;
	}
	
	#ls_infowindow_edge div {
			height: 150%;
			width: 100%;
			background-color: orange;
			border-right: 2px solid black;
			transform: rotate(45deg);
				-moz-transform: rotate(45deg);
				-ms-transform: rotate(45deg);
				-o-transform: rotate(45deg);
				-webkit-transform: rotate(45deg);
			transform-origin: 100% 0%;
				-moz-transform-origin: 100% 0%;
				-ms-transform-origin: 100% 0%;
				-o-transform-origin: 100% 0%;
				-webkit-transform-origin: 100% 0%;
		}

	#ls_infowindow_innercontainer_left {
		display: inline-block;
		height: 100%;
		width: 45%;
		vertical-align: top;
	}
	#ls_infowindow_innercontainer_right {
		display: inline-block;
		height: 100%;
		width: 54%;
		font-size: 0.85em;
	}

	#ls_infoWindow-title {
		height: 40%;
		margin: 0px;
		padding: 5px 5px 0px 10px;
		border-bottom: 2px solid #404040;
		box-sizing: border-box;
	}

	#ls_getParts_container {
		position: relative;
		width: 100%;
		height: 60%;
		padding: 0px 0px 0px 0px;
		cursor: pointer;
		transition: 0.3s ease background-color, color;
	}
	
		#ls_getParts_container:hover {
			background-color: #404040;
			color: white;
		}
		
		#ls_getParts_container:hover #ls_getParts_button_icon {
			filter: invert(100%);
		}
	
	#ls_getParts_button {
		position: absolute;
		left: 0;
		right: 0;
		top: 50%;
		transform: translateY(-50%);
		box-sizing: border-box;
		width: 150px;
		height: 30px;
		margin: auto;
		font-size: 1.4em;
		font-weight: bold;
	}	
		#ls_getParts_button_icon {
			display: inline-block;
			vertical-align: top;
			height: 100%;
			width: 25%;
			background-image: url("assets/list-icon.png");
			background-size: contain;
			background-repeat: no-repeat;
			margin: 0 7px 0 0;
		}
		#ls_getParts_button_title {
			display: inline-block;
			vertical-align: top;
			height: 100%;
			width: 70%;
		}
		

#ls_partsCount_container {
	overflow: hidden;
	height: 30px;
	padding: 6px 0px 0px 25px;
}
	
	#ls_partsCount {
		float: left;	
		width: 150px;
		height: 100%;
		margin: 0px;
		font-weight: normal;
	}
	
	.ls_partsCount_field {
		float: left;
		height: 100%;
		margin: 0;
		font-size: 1.5em;
		font-weight: bold;
	}
	
#ls_buildLength_container {
	overflow: hidden;
	height: 30px;
	padding: 0px 0px 0px 25px;
}
	
	#ls_buildLength {
		float: left;	
		width: 150px;
		height: 100%;
		margin: 0px;
		font-weight: normal;
	}
	
	.ls_buildLength_field {
		float: left;
		height: 100%;
		margin: 0;
		font-size: 1.5em;
		font-weight: bold;
	}
	
#ls_buildPrice_container {
	overflow: hidden;
	height: 30px;
	padding: 0px 0px 0px 25px;
}
	
	#ls_buildPrice {
		float: left;	
		width: 150px;
		height: 100%;
		margin: 0px;
		font-weight: normal;
	}
	
	.ls_buildPrice_field {
		float: left;
		height: 100%;
		margin: 0;
		font-size: 1.5em;
		font-weight: bold;
	}


/* 2.2 Menubuttons */
/*-----------------*/	
	
#ls_menubutton-container {
	position: absolute;
	right: 0px;
	top: 0px;
	width: 585px;
	height: 100px;
	overflow: hidden;
	z-index: 100;
}

	.ls_menubutton {
		width: 115px;
		height: 100%;
		float: right;
		margin-right: 30px;
		transform-origin: 0% 0%;
		box-sizing: border-box;
		cursor: pointer;
	}
	
	.ls_menubutton_bottom {
		width: 100%;
		height: 30px;
		overflow: hidden;
	}
		.ls_menubutton_bottom_left {
			float: left;
			width: 30px;
			height: 100%;
			overflow: hidden;
		}
			.ls_menubutton_bottom_left div {
				height: 200%;
				width: 200%;
				background-color: orange;
				transform: rotate(45deg);
				transform-origin: 128% 28%;
				box-sizing: border-box;
				border-bottom: 2px solid black;
			}
		.ls_menubutton_bottom_center {
			float: left;
			width: calc(100% - 60px);
			height: 100%;
			background-color: orange;
			border-bottom: 2px solid black;
			box-sizing: border-box;
		}
		.ls_menubutton_bottom_right {
			float: left;
			width: 30px;
			height: 100%;
			overflow: hidden;
		}
			.ls_menubutton_bottom_right div {
				height: 200%;
				width: 200%;
				background-color: orange;
				transform: rotate(45deg);
				transform-origin: 100% -50%;
				border-right: 2px solid black;
				box-sizing: border-box;
			}
			
#ls_menubutton_info {
	width: 17%
}

	.ls_menubutton_icon_container {
		position: relative;
		height: 35px;
		background-color: orange;
		border-left: 2px solid black;
		border-right: 2px solid black;
	}

	.ls_menubutton_icon {
		width: 100%;
		height: 35px;
		background-image: url("assets/info-icon.png");
		background-repeat: no-repeat;
		background-size: contain;
		background-position: center;
		position: absolute;
		top: 5px;
		left: 0px;
	}
	
	.ls_menubutton_title {
		width: 100%;
		height: calc(100% - 65px);
		padding: 14px 0px 0px 0px;
		border-left: 2px solid black;
		border-right: 2px solid black;
		background-color: orange;
		box-sizing: border-box;
		font-size: 1.1em;
		font-weight: bold;
		text-align: center;
	}
	
#ls_menubutton_background {
	width: 22%;
}

	#ls_menubutton_background .ls_menubutton_icon {
		background-image: url("assets/background-icon.png");
	}

#ls_menubutton_website {
	width: 40%;
}

	#ls_menubutton_website_title {
		width: 100%;
		height: 30px;
		text-align: center;
		font-size: 1.1em;
		box-sizing: border-box;
		padding: 5px 0px 0px 0px;
		font-weight: bold;
		background-color: orange;
		border-left: 2px solid black;
		border-right: 2px solid black;
	}
	
	#ls_menubutton_website_logo {
		height: calc(100% - 60px);
		width: 100%;
		background-image: url("assets/kickstarter.png");
		background-size: 85%;
		background-repeat: no-repeat;
		background-position: center;
		background-color: orange;
		border-left: 2px solid black;
		border-right: 2px solid black;
		box-sizing: border-box;
	}
	
/* 2.3 Mobile Menu */
/*-----------------*/

#ls_infowindowMobile {
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
}

	#ls_infowindowMobile h2 {
		float: left;		
		margin: 10px 0 0 20px;
		color: white;
		font-size: 1.1em;
	}

	#ls_infowindowMobile p {
		float: left;
		margin: 10px 0 0 10px;
		color: orange;
		font-size: 1.1em;
	}


#ls_mobile_menu {
	display: none;
	position: absolute;
	top: 0px;
	right: 0px;
	width: 100px;
	height: 100%;
	z-index: 100;
	background-image: url("assets/mobile-menu.png");
	background-repeat: no-repeat;
	background-size: contain;
	background-position: center;
	cursor: pointer;
}

	.ls_mobile_menu_open {
		background-color: orange;
		background-image: url("assets/mobile-menu-gray.png") !important;
	}

	#ls_mobile_menu_list {
		position: absolute;
		right: 0px;
		bottom: -202px;
		width: 200px;
		height: -200px;
		background-color: orange;
		overflow: hidden;
	}
	
	.ls_mobile_menu_item {
		float: right;
		height: 50px;
		width: 100%;
		padding: 15px 0px 0px 20px;
		font-size: 1.1em;
		font-weight: bold;
		box-sizing: border-box;
		border-bottom: 1px solid black;
	}

	

	
/* 2.4 Modbutton */
/*-----------------*/	
		
#ls_modbutton {		
	width: 260px;
	height: 60px;
	bottom: 0px;
	position: absolute;
	left: 0px;
	right: 0px;
	margin: auto;
}

	#ls_modbutton_center {
		position: relative;
		float: left;
		height: 100%;
		width: 140px;
		margin: 0;
		padding: 15 0 0 0;
		font-size: 1.5em;
		font-weight: bold;
		text-align: center;
		background-color: orange;
		border-top: 2px solid black;
		cursor: pointer;
		z-index: 100;
	}

	.ls_modbutton_edge {
		position: relative;
		float: left;
		width: 60px;
		height: 100%;
		overflow: hidden;
		z-index: 100;
	}
	
		#ls_modbutton_left div {
			height: 150px;
			width: 80px;
			background-color: orange;
			border-left: 2px solid black;
			transform: rotate(45deg);
				-moz-transform: rotate(45deg);
				-ms-transform: rotate(45deg);
				-o-transform: rotate(45deg);
				-webkit-transform: rotate(45deg);
			transform-origin: 136% 70%;
				-moz-transform-origin: 136% 70%;
				-ms-transform-origin: 136% 70%;
				-o-transform-origin: 136% 70%;
				-webkit-transform-origin: 136% 70%;			
		}
		
		#ls_modbutton_right div {
			height: 80px;
			width: 150px;
			background-color: orange;
			border-top: 2px solid black;
			transform: rotate(45deg);
				-moz-transform: rotate(45deg);
				-ms-transform: rotate(45deg);
				-o-transform: rotate(45deg);
				-webkit-transform: rotate(45deg);
			transform-origin: 1% 0%;
				-moz-transform-origin: 1% 0%;
				-ms-transform-origin: 1% 0%;
				-o-transform-origin: 1% 0%;
				-webkit-transform-origin: 1% 0%;
		}
		
	#ls_modbutton_backpanel {
		position: absolute;
		left: -120px;
		bottom: 0px;
		width: 500px;
		height: 40px;
		background-color: #404040;
		z-index: 50;
		border-radius: 20px 20px 0px 0px;
		border-top: 2px solid black;
	}

/* 2.5 Plus-Minus-Buttons */
/*-------------------------*/

.ls_buttonpanel {
	position: absolute;
	height: 40px;
	z-index: 10;
}

	#ls_buttonsleft {
		left: 20px;
		bottom: 0px;
	}
	
	#ls_buttonsright {
		right: 20px;
		bottom: 0px;
	}
	
.ls_button {
	display: inline-block;
	height: 40px;
	width: 40px;
	margin: 0;
}

	#ls_buttonleft_plus,
	#ls_buttonright_plus {
		background-image: url("assets/plus.png");
		background-size: 100% 100%;
	}
	
		#ls_buttonleft_plus {
			margin: 0 10px 0 0;
		}
		
		#ls_buttonright_plus {
			margin: 0 0 0 10px;
		}
	
	#ls_buttonleft_minus,
	#ls_buttonright_minus {
		background-image: url("assets/minus.png");
		background-size: 100% 100%;
	}

	
/* 3. Canvas */
/* --------------------------------------------------------------------------------- */

#ls_canvas {
	position: relative;
	height: calc(100% - 52px);
	overflow: hidden;
}

 #ls_paint_container {
 	height: 100%;
 }

.ls_canvas_container {
	position: absolute;
	top: 50%;
	left: 50%;
	right: 0;
	margin: 0;
	width: 0px;
	height: 300px;
	background-color: yellow;
}

#ls_preload_container {
	opacity: 0;
	z-index: -1000;
}

.ls_flip {
	-moz-transform: scaleX(-1);
  -o-transform: scaleX(-1);
  -webkit-transform: scaleX(-1);
  transform: scaleX(-1);
}

	#ls_select_container .ls_flip {
		-moz-transform: translateY(-50%) scaleX(-1);
	  -o-transform: translateY(-50%) scaleX(-1);
	  -webkit-transform: translateY(-50%) scaleX(-1);
	  transform: translateY(-50%) scaleX(-1);
	}

/* 4. Build parts */
/* --------------------------------------------------------------------------------- */

.ls_part {
	position: absolute;
	height: 100%;
}

.ls_imgcontainer {
	position: absolute;
	width: 100%;
	height: 100%;
}

.ls_img {
	position: absolute;
	top: 0px;
	left: 0px;
	max-height: 100%;
}
 
	#ls_build_container .ls_type_blade img {
		z-index: 1;
	}
	
	#ls_build_container .ls_type_shroud img {
		z-index: 15;
	}
	
	#ls_build_container .ls_type_emitter .ls_img {
		z-index: 5;
	}
	
	#ls_build_container .ls_type_chassis img,
	#ls_build_container .ls_type_chassis {
		z-index: 1;
	}
	
	#ls_build_container .ls_type_foregrip img {
		z-index: 10;
	}
	
	#ls_build_container .ls_type_reargrip img {
		z-index: 15;
	}
	
	#ls_build_container .ls_type_pommel img {
		z-index: 10;
	}
	
	#ls_build_container .ls_img_chassis_none {
		opacity: 0;
		transition: 1s ease opacity;
	}
	
#ls_paint_container #ls_build_container .ls_mod {
	position: absolute;
	max-height: 100%;
	z-index: 20;
}

 
/* 5. Select Screen */
/* --------------------------------------------------------------------------------- */
	
#ls_select_container {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	width: 1100px;
	height: 400px;
	margin: auto;
	background-color: rgba(0,0,0,0.7);
	z-index: 20;
}

	#ls_select_menu {
		width: 100%;
		height: 20%;
	}
		#ls_select_menu_topcat .ls_select_menu_active {
			background-color: orange;
		}
		
		#ls_select_menu_subcat .ls_select_menu_active {
			border-radius: 10px;
			background-color: orange;
		}
		
		#ls_select_menu_topcat {
			position: relative;
			width: 100%;
			height: 50%;
			overflow: hidden;
		}
			#ls_select_menu_topcat_title {
				float: left;
				width: 190px;
				height: 100%;
				margin: 0;
				padding: 5px 10px 0 10px;
				box-sizing: border-box;
				font-size: 1.5em;
				background-color: orange;
			}
			
			.ls_select_exit {
				position: absolute;
				right: 0;
				top: 0;
				height: 40px;
				width: 100px;
				padding: 0 0 0 40px;
				font-size: 2em;
				font-weight: bold;
				box-sizing: border-box;
				background-color: orange;
				cursor: pointer;
			}
			
			.ls_select_menu_topcat_item {
				float: left;
				height: 100%;
				margin: 0 0 0 5px;
				padding: 5px 10px 0 10px;
				font-size: 1.5em;
				color: white;
				cursor: pointer;
			}
			
		#ls_select_menu_subcat {
			width: 100%;
			height: 50%;
			overflow: hidden;
		}
			#ls_select_menu_subcat_arrow {
				float: left;
				height: 100%;
				width: 190px;
				border-radius: 0px 0px 10px 0px;
				background-image: url("assets/subarrow.png");
				background-size: contain;
				background-position: center;
				background-repeat: no-repeat;
				background-color: orange;	
			}
			
			.ls_select_menu_subcat_item {
				float: left;
				height: 25px;
				margin: 7px 0 0 5;
				padding: 3px 10px 0px 10px;
				font-size: 1.1em;
				color: white;
				cursor: pointer;
				border-radius: 10px;
			}			
	
	#ls_select_selection {
		height: 80%;
		padding: 10px 10px 20px 10px;
		overflow: auto;
		box-sizing: border-box;
	}

	.ls_select_itemcontainer {
		position: relative;
		display: inline-block;
		width: 262px;
		height: 90px;
		cursor: pointer;
		overflow: hidden;
		transition: 0.3s ease background-color;
	}
		.ls_select_itemcontainer:hover{
			background-color: rgba(150,150,150,0.5);
		}
 
	.ls_select_img {
		position: absolute;
		left: 0;
		right: 0;
		max-width: 90%;
		max-height: 125%;
		top: 45%;
		transform: translateY(-50%);
		margin: 0 auto;
	}
	
	.ls_select_item_title {
		position: absolute;
		bottom: 0px;
		width: 100%;
		margin: 0px;	
		padding: 0px 0px 3px 0px;
		font-size: 0.9em;
		text-align: center;
		color: white;
	}

#ls_select_inspectMenu {
	display: none;
	width: 100%;
	height: 15%;
	overflow: hidden;
}
	
		#ls_select_inspect_backButton {
			float: left;
			width: 80px;
			height: 100%;
			background-color: orange;
			font-weight: bold;
			font-size: 3em;
			text-align: center;
			cursor: pointer;
		}
	
		#ls_select_inspectMenubar {
			position: relative;
			float: left;
			height: 100%;
			width: calc(100% - 80px);
		}
			#ls_select_inspectMenubar h1 {
				height: 100%;
				margin: 0;
				padding: 10 0 0 20;
				color: white;
				box-sizing: border-box;
			}
	
#ls_select_inspectSelection {
	display: none;
	height: 85%;
	box-sizing: border-box;
	overflow: hidden;
}

#ls_select_inspect_modContainer .ls_select_inspect_selected {
	background-color: rgba(150,150,150,0.5);
}

#ls_select_inspect_colorContainer .ls_select_inspect_selected {
	border: 2px solid white;
}

#ls_select_inspect_modColorContainer .ls_select_inspect_selected {
	border: 2px solid white;
}

	#ls_select_inspect_leftPanel {
		float: left;
		height: 100%;
		width: 40%;
	}
	
		#ls_select_inspect_pictureContainer {
			position: relative;
			width: 100%;
			height: 50%;
		}
		
			#ls_select_inspect_pictureInnerContainer {
				position: absolute;
				left: 0px;
				right: 0px;
				max-width: 90%;
				height: 100%;
				margin: 0px auto;
			}
		
				#ls_select_inspect_image {
			    position: absolute;
			    top: 50%;
			    max-width: 100%;
			    max-height: 100%;
			    transform: translateY(-50%);
				}
				
				#ls_select_inspect_pictureInnerContainer .ls_flip {
					-moz-transform: translateY(-50%) scaleX(-1);
				  -o-transform: translateY(-50%) scaleX(-1);
				  -webkit-transform: translateY(-50%) scaleX(-1);
				  transform: translateY(-50%) scaleX(-1);
				}
				
				#ls_select_inspect_image_mod {
					position: absolute;
					top: 50%;
					max-height: 100%;
					transform: translateY(-50%);
				}

		
		#ls_select_inspect_colorContainer {
			position: relative;
			width: 100%;
