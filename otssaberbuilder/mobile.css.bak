/* Breakpoint 450px 
-------------------------*/
@media (max-width: 450px) {
	
	#ls_config #ls_modbutton {
		width: 100%;
	}
	
		#ls_config #ls_modbutton_backpanel {
			width: 100%;
			left: 0;
			border-radius: 0px;
		}
		
		#ls_config #ls_modbutton_center {
			width: 90px;
		}
		
		#ls_config #ls_modbutton_center_textset_large {
			display: none;
		}
		#ls_config #ls_modbutton_center_textset_small {
			display: inline;
		}
		
		#ls_config #ls_buttonsleft {
			left: 10px;
		}	
			#ls_config #ls_buttonleft_plus {
				margin: 0 5px 0 0;
			}
		#ls_config #ls_buttonsright {
			right: 10px;
		}	
			#ls_config #ls_buttonright_plus {
				margin: 0 0 0 5px;
			}
			
	#ls_config #ls_buildMenuMobile {
		width: 100px;
		padding: 5px 0px 0px 24px;
	}
		#ls_config .ls_buildMenuMobile_parts {
			display: block;
		}
		#ls_config .ls_buildMenuMobile_length {
			display: block;
		}
		#ls_config .ls_buildMenuMobile_price {
			display: block;
		}
	
	#ls_config #ls_infowindowMobile {
		display: none;
	}
	
	
	#ls_config #ls_mobile_menu {
		width: 50px;
	}
	
	#ls_config .ls_select_itemcontainer {
		width: 50%;
		height: 25%
	}
	
	#ls_config #ls_select_inspectMenubar {
		width: calc(100% - 40px);
	}
	
		#ls_config #ls_select_inspectMenubar h1 {
			padding: 5px 0px 0px 15px;
		}
		#ls_config #ls_select_inspect_backButton {
			width: 40px;
		}
		#ls_config .ls_select_exit {
			width: 40px;
			padding: 0 0 0 12px;
			font-size: 1.6em;
		}
		#ls_config #ls_select_inspectMenu .ls_select_exit {
			display: none;
		}
	
		#ls_config #ls_select_inspect_description_content {
			font-size: 0.8em;
		}
		
	
	#ls_config #ls_config_container {
		height: 120px;
	}
	
		#ls_config  #ls_config_container_left {
			float: none;
			position: absolute;
			bottom: 0px;
			width: 75%;
			height: 35%;
		}	
			#ls_config #ls_config_button_flip {
				display: inline-block;
				vertical-align: top;
				height: 100%;
				width: 50%;	
				padding: 8px 0px 5px 16px;	
			}
			#ls_config #ls_config_button_color {
				display: inline-block;
				vertical-align: top;
				height: 100%;
				width: 50%;
				padding: 5px 0px 5px 6px;
			}
		#ls_config  #ls_config_container_center {
			height: 60%;
			width: 75%;
		}
			#ls_config #ls_config_partName {
				font-size: 0.7em;
			}
		
			#ls_config #ls_config_partName_ok {
				float: none;
				position: absolute;
				right: 0px;
				top: 0px;
				width: 30px;
			}
			#ls_config #ls_config_partInfo_field {
				width: 100%;
			}
		#ls_config  #ls_config_container_right {
			width: 25%;
			padding: 5px 0px 0px 0px;
		}
		
			#ls_config #ls_config_modInfo {
				padding: 2px 0px 0px 10px;
			}
			
			#ls_config #ls_config_button_modColor {
				height: 40%;
				margin: 9px 0px 0px;
				padding: 4px 0px 0px 4px;
			}
	
	#ls_config #ls_randomizeUi {
		width: 100%;
	}
}

