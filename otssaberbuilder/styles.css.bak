/* 7. Dialogue window */
/* --------------------------------------------------------------------------------- */

#ls_dialogueBackground {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	z-index: 10000;
}

	#ls_dialogue_panel {
		position: absolute;
		top: 50%;
		transform: translateY(-50%);
		left: 0;
		right: 0;
		margin: auto;
		width: 400px;
		height: 450px;
		background-color: rgba(0,0,0,0.6);
		color: white;
		border-radius: 10px 10px 0px 0px;
	}
	
		#ls_dialogue_title {
			width: 100%;
			height: 25px;
			padding: 5px 0px;
			background-color: orange;
			text-align: center;
			font-size: 1.3em;
			font-weight: bold;
			color: black;
			border-radius: 10px 10px 0px 0px;
		}
		
		#ls_dialogue_content {
			overflow: auto;
			height: calc(100% - 90px);
			margin: 10px 0px;
		}
		
		#ls_dialogue_textcontainer {
			padding: 0px 20px;
		}
		
			.ls_dialogue_text {
				margin: 20px 0px 0px;
			}
			
		#ls_dialogue_partslist {
			padding: 10px 0px 0px;
		}
			
		.ls_dialogue_partslist_item {
			position: relative;
			width: 100%;
			font-size: 1.1em;
			padding: 0px 0px 0px 50px;
			box-sizing: border-box;
		}
		
		.ls_dialogue_partslist_item:before {
			position: absolute;
			display: inline-block;
			left: 14px;
			top: -32px;
			height: 20px;
			width: 20px;
			content: "\00B7";
			color: orange;
			font-size: 4em;
			text-align: center;
		}
		
			.ls_dialogue_partslist_part {
				height: 30px;
			}
			
			.ls_dialogue_partslist_mod {
				position: relative;
				height: 30px;
				padding: 0 0 0 50px;
			}
				.ls_dialogue_partslist_mod:before {
					position: absolute;
					display: inline-block;
					left: 0px;
					top: -2px;
					height: 20px;
					width: 53px;
					content: "";
					text-align: center;
					background-image: url("assets/subarrow-white.png");
					background-size: contain;
					background-repeat: no-repeat;
					background-position: center;
				}
				
		
		#ls_dialogue_button {
			position: absolute;
			bottom: 0px;
			left: 0px;
			right: 0px;
			width: 80%;
			height: 40px;
			margin: auto;
			text-align: center;
			font-size: 1.4em;
			font-weight: bold;
			cursor: pointer;
		}


